@{
    ViewBag.Title = "Car";
}
<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script src="../../Scripts/js_chart_style.js"></script>
<!--<script src="js/js_chart_data.js"></script>-->

<script>
	$(function() {
		Highcharts.chart('work_chart', {
		chart: {
			type: 'column'
		},
		title: {
			text: 'Work Year'
		},
		xAxis: {
			categories: [
			'Jan',
			'Feb',
			'Mar',
			'Apr',
			'May',
			'Jun',
			'Jul',
			'Aug',
			'Sep',
			'Oct',
			'Nov',
			'Dec'
			]
		},
		yAxis: [{
			min: 0,
			title: {
			text: 'Days'
			}
		}, {
			title: {
			text: ''
			},
			opposite: true
		}],
		legend: {
			shadow: false
		},
		tooltip: {
			shared: true
		},
		plotOptions: {
			column: {
			grouping: false,
			shadow: false,
			borderWidth: 0
			}
		},
		series: [{
			name: 'Days in month',
			color: '#3a4352',
			data: [31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31],
			pointPadding: 0.2
		}, {
			name: 'Work Days',
			color: '#00b4d5',
			<!-- data: {get_data_from_DB} -->
			data: [20, 18, 19, 17, 21, 20, 21, 16, 18, 19, 20, 17],
			pointPadding: 0.2
		}]
		});
	});
</script>


		
<div class="top_cars paralax">
